<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN"
   "http://www.w3.org/TR/html4/strict.dtd">

<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>ACE autocomplete</title>
    <meta name="author" content="Rafael Xavier">
    <meta name="copyright" content="Varun Ganapathi">
    <link rel="stylesheet" href="ace_autocomplete.css" type="text/css" media="screen" charset="utf-8">
    <style>
    #autocomplete_outer { 
      display:none;
      outline:none;
      z-index: 1000;
    }
    #autocomplete_list {
      display:inline-block;
      background-color:white;
      padding:0px;
      max-height:120px;
      margin-bottom:1em;
      border:1px solid grey;
      scroll:auto;
      overflow-y:scroll;
      font-family:monospace,fixed;
      cursor:default;
      color: black;
    }
    .autocomplete_text {
      color: black;
      font-family: monospace;
      text-align:left;
    }

    ul.autocomplete {
      display: inline-block;
      list-style-type: none;
      padding:0;
      margin:0;
    }

    ul.autocomplete li {
      background-color:white;
      padding-left:1px;
      padding-right:5px;
      border: 1px solid white;
      font-family:monospace;
    }
    ul.autocomplete li.selected {
      background-color:lightgrey;
    }
    ul.autocomplete li.hover {
      border: 1px solid grey;
    }
    </style>
</head>
<body>
  <div id="editor"></div>
  <script type="text/javascript">
      var require = {
          baseUrl: window.location.protocol + "//" + window.location.host + window.location.pathname.split("/").slice(0, -1).join("/"),
          paths: {
              src: "../src"
          }
      };
  </script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
  <script src="require.js" data-main="ace_autocomplete" type="text/javascript"></script>
  <script defer src="../libs/ace.js" type="text/javascript"></script>
  <script defer src="../libs/theme-textmate.js" type="text/javascript"></script>
  <script defer src="../libs/mode-javascript.js" type="text/javascript"></script>
</body>
</html>
